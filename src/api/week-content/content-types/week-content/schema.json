{
  "kind": "collectionType",
  "collectionName": "week_contents",
  "info": {
    "singularName": "week-content",
    "pluralName": "week-contents",
    "displayName": "Week",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "weekId": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource"
    },
    "forms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::form.form",
      "inversedBy": "weeks"
    },
    "week_number": {
      "type": "integer"
    },
    "week_status": {
      "type": "enumeration",
      "enum": [
        "closed",
        "open",
        "upcoming"
      ]
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "week"
    },
    "answers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::answer.answer",
      "mappedBy": "weeks"
    },
    "related_resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "week"
    }
  }
}
