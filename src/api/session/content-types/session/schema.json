{
  "kind": "collectionType",
  "collectionName": "sessions",
  "info": {
    "singularName": "session",
    "pluralName": "sessions",
    "displayName": "Session"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "time": {
      "type": "enumeration",
      "enum": [
        "morning",
        "evening"
      ]
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "sessions"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "sessions"
    }
  }
}
